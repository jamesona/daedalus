{
	"compileOnSave": true,
	"compilerOptions": {
		"outDir": "./dist/",
		"baseUrl": "./src",
		"sourceMap": true,
		"declaration": false,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"moduleResolution": "node",
		"module": "es6",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2017",
			"dom"
		],
	}
}
