{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "'./src/",
		"pretty": true,
		"newLine": "LF",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"target": "es5"
	}
}
